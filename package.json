{
  "name": "wpchill-kb",
  "version": "1.0.0",
  "description": "WPChill Knowledge Base Plugin",
  "scripts": {
    "sass:watch": "sass --watch /assets/scss:assets/css",
    "sass:compile": "sass assets/scss/wpchill-kb.scss assets/css/wpchill-kb-styles.css --style compressed",
    "start": "npm run sass:watch",
    "build": "npm run sass:compile"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "knowledge base"
  ],
  "author": "WPChill",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "sass": "^1.62.1"
  }
}